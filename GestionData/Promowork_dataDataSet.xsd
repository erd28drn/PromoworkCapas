                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     ISNULL(ISNULL(HorasTrabajadas_1.IdTrabajador, SeguridadSocial.IdTrabajador), BajasMedica_1.IdTrabajdor) AS IdTrabajador, ISNULL(HorasTrabajadas_1.IdObra, 0) 
