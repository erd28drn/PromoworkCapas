                      Obras ON HorasTrabajadas_1.IdObra = Obras.IdObra LEFT OUTER JOIN
                      Tipos ON HorasTrabajadas_1.IdTipo = Tipos.IdTipo FULL OUTER JOIN
                      BajasMedica ON HorasTrabajadas_1.IdTrabajador = BajasMedica.IdTrabajdor AND HorasTrabajadas_1.MesTrab = BajasMedica.MesBaja AND 
                      HorasTrabajadas_1.AnoTrab = BajasMedica.AnoBaja FULL OUTER JOIN
                      SeguridadSocial ON HorasTrabajadas_1.AnoTrab = SeguridadSocial.AnoSegSocial AND HorasTrabajadas_1.MesTrab = SeguridadSocial.MesSegSocial AND 
                      HorasTrabajadas_1.IdTrabajador = SeguridadSocial.IdTrabajador
