          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ResumenObraCompletaAdminTableAdapter" GeneratorDataComponentClassName="ResumenObraCompletaAdminTableAdapter" Name="ResumenObraCompletaAdmin" UserDataComponentName="ResumenObraCompletaAdminTableAdapter">
