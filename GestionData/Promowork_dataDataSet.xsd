                         Trabajadores AS Trabajadores_1 ON HorasTrabajadas_1.IdTrabajador = Trabajadores_1.IdTrabajador AND HorasTrabajadas_4.MesTrab = @nMes AND 
                         HorasTrabajadas_4.AnoTrab = @nAno INNER JOIN
                         Obras AS Obras_1 ON HorasTrabajadas_1.IdObra = Obras_1.IdObra LEFT OUTER JOIN
                         CrucesTrabajadores AS CrucesTrabajadores_1 ON HorasTrabajadas_1.IdTrabajador = CrucesTrabajadores_1.IdTrabCruce AND 
