                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        0 AS ComSal, Obras.IdObra, Obras.IdEmpresa, Obras.NumObra, Obras.DesObra, Obras.FechaIniObra, Obras.FechaFinObra, Clientes.NumCliente, 
                         Clientes.DesCliente, 0 AS IdCompra, NULL AS Factura, NULL AS FechaFactura, 0 AS ImpBase, 0 AS ImpIva, 0 AS idProveedor, 0 AS NumProveedor, NULL 
                         AS DesProveedor, HorasTrabajadas_1.IdTrabajador, dbo.ObtieneFecha(HorasTrabajadas_1.AnoTrab, HorasTrabajadas_1.MesTrab, HorasTrabajadas_1.DiaTrab) 
                         AS FechaHoras, HorasTrabajadas_1.DiaTrab, HorasTrabajadas_1.MesTrab, HorasTrabajadas_1.AnoTrab, HorasTrabajadas_1.IdTipo, HorasTrabajadas_1.Festivo, 
                         HorasTrabajadas_1.CantAdmin AS Cantidad, HorasTrabajadas_1.Salario, Trabajadores.DNITrabajador, Trabajadores.NumTrabajador, Trabajadores.NomTrabajador, 
                         Trabajadores.ApeTrabajador, Trabajadores.IdCategoria, Categorias.DesCategoria, Tipos.DesTipo, ISNULL(SalariosVentaAdmin.SalVenta, 
                         HorasTrabajadas_1.SalarioVenta) AS SalVenta, HorasTrabajadas_1.DesTrabajo, NULL AS FechaExp, NULL AS FechaRec, NULL AS FechaAbono, NULL 
                         AS FechaVcto, NULL AS FactCob, NULL AS FechaFactCob, NULL AS DesFormaPago, NULL AS DesBanco, NULL AS NumCuenta, 0 AS ImpBaseCob, 
                         0 AS ImpIVACob, NULL AS Referencia, NULL AS IdCobro, HorasTrabajadas_1.Color AS ColorHoras, 0 AS ColorCompra, HorasTrabajadas_1.IdHoras, 
                         HorasTrabajadas_1.ObsHoras AS Observ, 0 AS IdDetCompra
FROM            HorasTrabajadas AS HorasTrabajadas_1 INNER JOIN
                         Trabajadores ON HorasTrabajadas_1.IdTrabajador = Trabajadores.IdTrabajador INNER JOIN
                         Obras ON HorasTrabajadas_1.IdObra = Obras.IdObra INNER JOIN
                         Tipos ON HorasTrabajadas_1.IdTipo = Tipos.IdTipo LEFT OUTER JOIN
                         SalariosVenta ON HorasTrabajadas_1.IdTipo = SalariosVenta.IdTipo AND HorasTrabajadas_1.IdObra = SalariosVenta.IdObra AND 
                         Trabajadores.IdCategoria = SalariosVenta.IdCategoria LEFT OUTER JOIN
                         Clientes ON Obras.IdCliente = Clientes.IdCliente LEFT OUTER JOIN
                         SalariosVentaAdmin ON Trabajadores.IdCategoria = SalariosVentaAdmin.IdCategoria AND SalariosVentaAdmin.IdObra = HorasTrabajadas_1.IdObra AND 
                         SalariosVentaAdmin.IdTipo = HorasTrabajadas_1.IdTipo AND dbo.ObtieneFecha(HorasTrabajadas_1.AnoTrab, HorasTrabajadas_1.MesTrab, 
                         HorasTrabajadas_1.DiaTrab) BETWEEN SalariosVentaAdmin.FechaIni AND SalariosVentaAdmin.FechaFin LEFT OUTER JOIN
                         Categorias ON Trabajadores.IdCategoria = Categorias.IdCategoria
WHERE        (Obras.IdObra = @nIdObra) AND (dbo.ObtieneFecha(HorasTrabajadas_1.AnoTrab, HorasTrabajadas_1.MesTrab, HorasTrabajadas_1.DiaTrab) BETWEEN 
                         @FechainiHoras AND @FechafinHoras) AND (HorasTrabajadas_1.CantAdmin &lt;&gt; 0) AND (HorasTrabajadas_1.Color = @colorRojo OR
                         HorasTrabajadas_1.Color = @colorAzul) OR
                         (Obras.IdObra = @nIdObra) AND (dbo.ObtieneFecha(HorasTrabajadas_1.AnoTrab, HorasTrabajadas_1.MesTrab, HorasTrabajadas_1.DiaTrab) BETWEEN 
                         @FechainiHoras AND @FechafinHoras) AND (HorasTrabajadas_1.CantAdmin &lt;&gt; 0) AND (ISNULL(HorasTrabajadas_1.Color, - 16777216) = @colorNegro)
UNION ALL
SELECT        1 AS ComSal, Obras_1.IdObra, Obras_1.IdEmpresa, Obras_1.NumObra, Obras_1.DesObra, Obras_1.FechaIniObra, Obras_1.FechaFinObra, Clientes_1.NumCliente, 
                         Clientes_1.DesCliente, ComprasCab.IdCompra, ComprasCab.Factura, ComprasCab.FechaFactura, ComprasDet.ImpBase, ComprasDet.ImpIva, 
                         ComprasCab.idProveedor, Proveedores.NumProveedor, Proveedores.DesProveedor, 0 AS IdTrabajador, NULL AS FechaHoras, 0 AS DiaTrab, 0 AS MesTrab, 
                         0 AS AnoTrab, 0 AS IdTipo, NULL AS Festivo, 0 AS Cantidad, 0 AS Salario, NULL AS DNITrabajador, 0 AS NumTrabajador, NULL AS NomTrabajador, NULL 
                         AS ApeTrabajador, 0 AS IdCategoria, NULL AS DesCategoria, NULL AS DesTipo, 0 AS SalVenta, ComprasCab.Observ AS ObsHorasTrabajadas, NULL 
                         AS FechaExp, NULL AS FechaRec, NULL AS FechaAbono, NULL AS FechaVcto, NULL AS FactCob, NULL AS FechaFactCob, NULL AS DesFormaPago, NULL 
                         AS DesBanco, NULL AS NumCuenta, 0 AS ImpBaseCob, 0 AS ImpIVACob, NULL AS Referencia, NULL AS IdCobro, 0 AS ColorHoras, 
                         ComprasDet.Color AS ColorCompra, 0 AS IdHorasTrabajadas, ComprasDet.ObsCompra AS Observ, ComprasDet.IdDetCompra
FROM            ComprasDet INNER JOIN
                         Obras AS Obras_1 ON ComprasDet.IdObra = Obras_1.IdObra INNER JOIN
                         ComprasCab ON ComprasDet.IdCompra = ComprasCab.IdCompra INNER JOIN
                         Proveedores ON ComprasCab.idProveedor = Proveedores.IdProveedor LEFT OUTER JOIN
                         Clientes AS Clientes_1 ON Obras_1.IdCliente = Clientes_1.IdCliente
WHERE        (Obras_1.IdObra = @nIdObra) AND (ComprasCab.FechaFactura BETWEEN @FechainiCompras AND @FechafinCompras) AND (ComprasDet.Color = @colorRojo OR
                         ComprasDet.Color = @colorAzul) OR
                         (Obras_1.IdObra = @nIdObra) AND (ComprasCab.FechaFactura BETWEEN @FechainiCompras AND @FechafinCompras) AND (ISNULL(ComprasDet.Color, - 16777216) 
                         = @colorNegro)
UNION ALL
SELECT        2 AS ComSal, Obras_2.IdObra, Obras_2.IdEmpresa, Obras_2.NumObra, Obras_2.DesObra, Obras_2.FechaIniObra, Obras_2.FechaFinObra, Clientes_2.NumCliente, 
