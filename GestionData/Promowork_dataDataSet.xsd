                         AS DesProveedor, 0 AS IdTrabajador, NULL AS FechaHoras, 0 AS DiaTrab, 0 AS MesTrab, 0 AS AnoTrab, 0 AS IdTipo, Cobros.Prevision AS Festivo, 0 AS Cantidad, 
                         0 AS Salario, NULL AS DNITrabajador, 0 AS NumTrabajador, NULL AS NomTrabajador, NULL AS ApeTrabajador, 0 AS IdCategoria, NULL AS DesCategoria, NULL 
                         AS DesTipo, 0 AS SalVenta, NULL AS ObsHorasTrabajadas, Cobros.FechaExp, Cobros.FechaRec, Cobros.FechaAbono, Cobros.FechaVcto, ISNULL(CONVERT(varchar, 
                         FacturasCab.NumFactura) + '/' + CONVERT(char(4), DATEPART(year, FacturasCab.FechaFactura)), Cobros.Factura) AS FactCob, ISNULL(FacturasCab.FechaFactura, 
                         Cobros.FechaFactura) AS FechaFactCob, FormasPago.DesFormaPago, Bancos.DesBanco, Cuentas.NumCuenta, Cobros.ImpBase AS ImpBaseCob, 
