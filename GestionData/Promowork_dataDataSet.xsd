                                                          HorasTrabajadas AS HorasTrabajadas_1 ON CrucesTrabajadores_1.IdTrabCruce = HorasTrabajadas_1.IdTrabajador AND 
                                                          CrucesTrabajadores_1.DiaCruce = HorasTrabajadas_1.DiaTrab AND CrucesTrabajadores_1.MesCruce = HorasTrabajadas_1.MesTrab AND 
