                         Obras AS Obras_2 ON HorasTrabajadas_4.IdObra = Obras_2.IdObra INNER JOIN
                         Trabajadores AS Trabajadores_2 ON HorasTrabajadas_4.IdTrabajador = Trabajadores_2.IdTrabajador INNER JOIN
