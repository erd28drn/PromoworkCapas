                         CrucesTrabajadores ON Trabajadores_1.IdTrabajador = CrucesTrabajadores.IdTrabCruce INNER JOIN
                         HorasTrabajadas INNER JOIN
                         Trabajadores ON HorasTrabajadas.IdTrabajador = Trabajadores.IdTrabajador INNER JOIN
                         Obras ON HorasTrabajadas.IdObra = Obras.IdObra ON CrucesTrabajadores.IdTrabajador = HorasTrabajadas.IdTrabajador AND 
