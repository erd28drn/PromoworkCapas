                      SeguridadSocial.ImpSegSocila, SeguridadSocial.ImpIRPF, ISNULL(HorasTrabajadas_1.Cantidad * HorasTrabajadas_1.Salario /
                          (SELECT     SUM(Cantidad * Salario) AS TotalObra
                            FROM          HorasTrabajadas
                            WHERE      (MesTrab = HorasTrabajadas_1.MesTrab) AND (AnoTrab = HorasTrabajadas_1.AnoTrab) AND (IdTrabajador = HorasTrabajadas_1.IdTrabajador)), 1) 
